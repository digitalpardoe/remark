<%= form_for :setting, :url => { :action => "update" }, :html => { :method => :put } do |f| %>
  <% for setting in @settings %>
    <fieldset>
      <%= f.label setting.id, setting.human_readable, :class => 'block' %>
      <%= field_for(setting, f) %>
    </fieldset>
  <% end %>
  
  <fieldset>
    <%= f.submit "Save" %>
  </fieldset>
<% end %>
